{
  "name": "YouTube Fill Screen",
  "description": "Resize YouTube video to fill screen",

  "version": "0.0.0.1",
  "manifest_version": 2,

  "permissions": [
    "activeTab","tabs", "https://*/*", "http://*/*"
  ],
  
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  
  "permissions": [
    "tabs", "storage"
  ],
  
  "browser_action": {
  	"default_icon": "icon.png",
    "default_title": "Page Refresh"
  }
}
